generator client {
   provider = "prisma-client-js"
}

datasource db {
   provider  = "postgresql"
   url       = env("DATABASE_URL")
   directUrl = env("DIRECT_URL")
}

model Testimonial {
   id          String  @id @default(cuid())
   personName  String
   personRole  String
   description String
   isHighlight Boolean
   isActive    Boolean

   createdAt DateTime @default(now())
}
